    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "_commands": {
    "0": "npm i -g @nestjs/cli",
    "1": "nest new notifications-service",
    "2": "npm i prisma -D",
    "3": "npm i @prisma/client",
    "4": "npx prisma init --datasource-provider SQLite",
    "5": "npx prisma migrate dev",
    "6": "npm i class-validator class-transformer",
    "7": "npm uninstall eslint eslint-config-prettier eslint-plugin-prettier prettier"
  },
